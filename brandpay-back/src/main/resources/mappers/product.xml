<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.github.niwin92.brandpay.product.ProductMapper">

    <select id="findAll" resultType="io.github.niwin92.brandpay.product.ProductDto">
        SELECT * FROM PRODUCT
    </select>

    <select id="findById" parameterType="String" resultType="io.github.niwin92.brandpay.product.ProductDto">
        SELECT * FROM PRODUCT
        WHERE PRD_ID = #{prdId}
    </select>

</mapper>